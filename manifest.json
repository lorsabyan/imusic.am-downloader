{
  "name": "iMusic Downloder",
  "description": "Allows to download from iMusic.am",
  "version": "3.1",
  "permissions": [ "activeTab", "https://imusic.am/*", "https://stream.imusic.am/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Show me the download links"
  },
  "manifest_version": 2
}